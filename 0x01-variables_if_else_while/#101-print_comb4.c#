#include<stdio.h>

/**
 * main - Entry point
 *
 * Description: 'print numbers separated by , and combine three'
 *
 * Return: Always 0 (Success)
 */

int main(void)
{
int a, b, c, d;
c = 0;

for (a = c ; a < 8 ; a++)
{
for (b = a + 1 ; b < 8 ; b++)
{
for (d = b + 1 ; d < 10 ; d++)
{
putchar(a % 10 + '0');
putchar(b % 10 + '0');
putchar(d % 10 + '0');
if (a == 7 && b == 8)
break;
putchar(',');
putchar(' ');
}
c = c + 1;
}

putchar('\n');
return (0);
}
